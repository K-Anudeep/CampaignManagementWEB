@model IEnumerable<MarketingManagement.WEBUI.Models.CampaignDetails>

@{ ViewData["Title"] = "GetCampaignAsgnToMe";
                Layout = "~/Views/ExecutiveHome/_Layout.cshtml"; }
<script>$(document).ready(function () {

        $("#createbtn").click(function () {
            var credentials = {
                "CampaignId": $('#CampaignId').val(),
                "Name": $('#Name').val(),
                "Venue": $('#Venue').val(),
                "AssignedTo": $('#AssignedTo').val(),
                "StartedOn": $('#StartedOn').val(),
                "CompletedOn": $('#CompletedOn').val(),

            };
            $.ajax({
                type: "GET",
                url: "http://localhost:25733/api/Executive/Campaigns/AssignedToMe",
                data: JSON.stringify(credentials),
                contentType: "application/json",
                success: function (response) {
                    alert("Success")
                },
                error: function () {
                    alert('Error')
                }
            })
        })
    })</script>


